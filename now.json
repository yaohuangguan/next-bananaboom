{
  "name": "next-bananaboom",
  "version":2,
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "api": "https://nextbananaboom.herokuapp.com",
  "headers": [
    {
      "source": "/(.*)",
      "headers" : [
        {
          "key" : "X-Content-Type-Options",
          "value" : "nosniff"
        },
        {
          "key" : "X-Frame-Options",
          "value" : "DENY"
        },
        {
          "key" : "X-XSS-Protection",
          "value" : "1; mode=block"
        }
      ]
    }
  ],
  "routes": [
    { "src": "/shopping", "dest": "https://yby.best" }
  ]

}