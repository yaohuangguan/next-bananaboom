{
  "name": "next-bananaboom",
  "version": 2,
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "headers":[
    {
      "source": "/(.*)",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
