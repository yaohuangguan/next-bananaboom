{
  "name": "next-bananaboom",
  "version": 2,
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "headers":[
    {
      "source": "/(.*)",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "no-cache"
        }
      ]
    }
  ],
  "routes": [
    { "src": "/shopping", "dest": "https://yby.best" },
    {
      "src": "^/sw.js",
      "headers": { "cache-control": "s-maxage=0" },
      "dest": "/sw.js"
    }
  ]
}
