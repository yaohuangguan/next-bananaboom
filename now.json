{
  "name": "next-bananaboom",
  "version": 2,
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Cache-Control",
          "value": "s-maxage=1, stale-while-revalidate"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; img-src *; media-src 'self'; script-src 'self'; style-src *; font-src *; frame-src 'self'; "
        }
      ]
    }
  ]
}
